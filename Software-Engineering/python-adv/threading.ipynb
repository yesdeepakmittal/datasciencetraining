    "from threading import Thread\n",
    "\n",
    "def name(name):\n",
    "    print('My name is',name)\n",
    "\n",
    "t1 = Thread(target=name,args=('Elon',))\n",
    "t2 = Thread(target=name,args=('Musk',))\n",
    "\n",
    "t1.start()\n",
    "t2.start()\n",
    "\n",
    "t1.join() \n",
    "t2.join()"
   ]
  }
